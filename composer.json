{
	"name": "nextcloud/dkmunicipalorganisation",
	"description": "Integration to the Danish Inter-municipal infrastructure for organisation and access control",
	"license": "AGPL-3.0-or-later",
	"authors": [
		{
			"name": "Lamotech Solutions ApS",
			"email": "dev@lamotech.dk",
			"homepage": "https://lamotech.dk"
		}
	],
	"autoload": {
		"psr-4": {
			"OCA\\DkMunicipalOrganisation\\": "lib/"
		}
	},
	"provide": {
		"psr/log-implementation": "3.0.0",
		"psr/container-implementation": "2.0.0",
		"psr/event-dispatcher-implementation": "1.0.0",
		"psr/clock-implementation": "1.0.0"
	},
	"replace": {
		"psr/log": "*",
		"psr/container": "*",
		"psr/event-dispatcher": "*",
		"psr/clock": "*"
	},
	"scripts": {
		"post-install-cmd": [
			"@composer bin all install --ansi"
		],
		"post-update-cmd": [
			"@composer bin all install --ansi"
		],
		"lint": "find . -name \\*.php -not -path './vendor/*' -not -path './vendor-bin/*' -not -path './build/*' -print0 | xargs -0 -n1 php -l",
		"cs:check": "php-cs-fixer fix --dry-run --diff",
		"cs:fix": "php-cs-fixer fix",
		"psalm": "psalm --threads=1 --no-cache",
		"test:unit": "phpunit tests -c tests/phpunit.xml --colors=always --fail-on-warning --fail-on-risky",
		"openapi": "generate-spec",
		"rector": "rector && composer cs:fix"
	},
	"require": {
		"bamarni/composer-bin-plugin": "^1.8",
		"php": "^8.1",
		"onelogin/php-saml": "^4.2",
		"phpseclib/phpseclib": "^3.0",
		"robrichards/xmlseclibs": "^3.1.4"
	},
	"require-dev": {
		"nextcloud/ocp": "dev-stable30",
		"roave/security-advisories": "dev-latest"
	},
	"config": {
		"allow-plugins": {
			"bamarni/composer-bin-plugin": true
		},
		"optimize-autoloader": true,
		"sort-packages": true,
		"platform": {
			"php": "8.1"
		}
	}
}
