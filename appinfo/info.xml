<?xml version="1.0"?>
<info xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	  xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
	<id>dkmunicipalorganisation</id>
	<name>DK Municipal Organisation and Access Control</name>
	<summary>Danish Municipal Organisation and Access Control Integration</summary>
	<description>Integration to the Danish Inter-municipal infrastructure for organisation and access control</description>
	<version>0.3.00</version>
	<licence>agpl</licence>
	<author mail="dev@lamotech.dk" homepage="https://lamotech.dk">Lamotech Solutions ApS</author>
	<namespace>DkMunicipalOrganisation</namespace>
	<category>organisation</category>
	<category>security</category>
	<bugs>https://lamotech.dk</bugs>
	<dependencies>
		<nextcloud min-version="32" max-version="33"/>
	</dependencies>
	<commands>
		<command>OCA\DkMunicipalOrganisation\Command\FetchOrganisationsCommand</command>
		<command>OCA\DkMunicipalOrganisation\Command\GenerateSAMLMetadataCommand</command>
		<command>OCA\DkMunicipalOrganisation\Command\IssueTokenCommand</command>
		<command>OCA\DkMunicipalOrganisation\Command\RegisterCertificateCommand</command>
		<command>OCA\DkMunicipalOrganisation\Command\ShowSyncLogCommand</command>
		<command>OCA\DkMunicipalOrganisation\Command\SyncOrganisationsCommand</command>
		<command>OCA\DkMunicipalOrganisation\Command\ValidateOrganisationsCommand</command>
	</commands>
	<settings>
		<admin-section>OCA\DkMunicipalOrganisation\Settings\AdminSection</admin-section>
		<admin>OCA\DkMunicipalOrganisation\Settings\AdminSettings</admin>
	</settings>
	<backgroundjobs>
		<backgroundjob>OCA\DkMunicipalOrganisation\BackgroundJob\SyncOrganisationsJob</backgroundjob>
	</backgroundjobs>
</info>
